{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [
    "token",
    "fileName",
    "attributes"
  ],
  "properties": {
    "token": {
      "type": "string",
      "minLength": 1
    },
    "fileName": {
      "type": "string",
      "minLength": 40,
      "maxLength": 40
    },
    "attributes": {
      "type": "object",
      "required": [
        "price",
        "style",
        "colors",
        "sizes"
      ],
      "properties": {
        "price": {
          "type": "integer"
        },
        "style": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "colors": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "minLength": 1
          }
        },
        "sizes": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    }
  }
}