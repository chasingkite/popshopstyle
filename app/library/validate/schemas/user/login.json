{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "username",
        "email",
        "facebookToken"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 5,
            "maxLength": 32
        },
        "email": {
            "type": "string",
            "format": "email",
            "minLength": 5,
            "maxLength": 32
        },
        "facebookToken": {
            "type": "string",
            "minLength": 150
        }
    }
}
